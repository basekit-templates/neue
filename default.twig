{% extends "layout.twig" %}

{% block content %}

<div class="ecom-nav">
    <div class="container">
        <div class="grid">
            <nav class="col-60">
                {{widget('extendednavigation', 'desktopnavigation', {})|raw}}
            </nav><!--end col-60-->
            <div class="col-30">
                {{widget('ecombasket', 'ecombasket', {})|raw}}
            </div><!--end col-30-->
        </div><!--end grid-->
    </div><!--end container-->
</div><!--end ecom-nav-->

<div class="offset">

    <header class="bk-header">
        <div class="container">
            <div class="grid">
                <div class="col-30">   
                </div><!--end col-50-->
                <div class="col-30">
                    {{widget('companyname', 'companyname', {"business": "NEUE"})|raw}}
                </div><!--end col-30-->
                <div class="col-30">
                    {{widget('clicktocall', 'thisunqiuewidgetname', {phoneText: 'Click to Call'})|raw}}
                </div><!--end col-30-->
            </div><!--end grid-->
        </div><!--end container-->
    </header>

    <div role="main">
        <div class="container">
            <div class="grid">
                <div class="col-100">
                    {{zone('main')|raw}}
                </div><!--end 100-->
            </div><!--end grid-->
        </div><!--end container-->
    </div><!--end main-->

    <footer class="bk-footer">
        <div class="container">
            <div class="grid">
                <div class="col-30">
                    {{widget('companyname', 'companyname', {"business": "NEUE"})|raw}}
                </div><!--end col-30-->
                <div class="col-15">
                    {{widget('content', 'copyright', {content: 'Lorem ipsum dolor sit amet, consectetur adipiscing elit.', lines: 'all'})|raw}}
                </div><!--end col-30-->
                <div class="col-15">
                    {{widget('companyaddress', 'address', {})|raw}}
                </div><!--end col-30-->
                <div class="col-30">
                    {{widget('signupform', 'signup-footer', {'text': 'Send', 'label': 'Your Email', 'formTitle': '', 'email': 'profile'})|raw}}
                </div><!--end col-30-->
            </div><!--end grid-->
            <div class="grid copyright">
                <div class="col-50">
                    {{widget('content', 'copyright', {content: 'Â© Lorum ipsum dolor', lines: 'all'})|raw}}
                </div><!--end col-50-->
                <div class="col-50">   
                    {{widget('socialicons', 'social', {'googleplus': 'https://plus.google.com/101946323226233111530', 'linkedin':'http://www.linkedin.com/company/basekit', 'facebook': 'https://www.facebook.com/TeamBaseKit', 'twitter': 'https://twitter.com/basekit', 'youtube': 'https://www.youtube.com/user/TeamBaseKit', 'rss': 'http://blogdiseno.basekit.es/feed/'})|raw}}
                </div><!--end col-50-->
            </div><!--end grid-->
    </footer>

</div><!--end offset-->

{% endblock %}