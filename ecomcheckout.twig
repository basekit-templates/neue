{% extends "layout.twig" %}

{% block content %}

<div class="ecom-nav">
    <div class="container">
        <div class="grid">
            <nav class="col-100 desktop-nav">
                {{widget('extendednavigation', 'desktopnav', {"align":"left"})|raw}}
            </nav><!--end col-100-->
        </div><!--end grid-->
    </div><!--end container-->
</div><!--end ecom-nav-->

<header class="bk-header">
    <div class="container">
        <div class="grid">
            <div class="col-100">
                {{widget('companyname', 'companyname', {"business": "profile"})|raw}}
            </div><!--end col-100-->
        </div><!--end grid-->
    </div><!--end container-->
</header>

<div role="main">
    <div class="container">
        <div class="grid">
            <div class="col-100">
                {{zone('main')|raw}}
            </div><!--end 100-->
        </div><!--end grid-->
    </div><!--end container-->
</div><!--end main-->

{% include 'includes/footer.twig' %}

{% endblock %}